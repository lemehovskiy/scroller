{
  "name": "@lemehovskiy/scroller",
  "version": "0.0.1",
  "description": "VannilaJS plugin for detecting vertical scroll progress",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.demo.js",
    "build": "webpack && tsc",
    "build:demo": "webpack --config webpack.config.demo.js",
    "test": "jest --silent",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemehovskiy/scroller"
  },
  "author": "lemehovskiy <lemehovskiy@gmail.com> (https://github.com/lemehovskiy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lemehovskiy/scroller/issues"
  },
  "homepage": "https://github.com/lemehovskiy/scroller",
  "keywords": [
    "scroll",
    "plugin",
    "typescript"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.8",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-minify": "^0.5.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.2",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.5.1",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "4.7.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "dependencies": {
    "@lemehovskiy/scroller-utils": "file:../scroller-utils"
  }
}
